    def process_tle_orbital_calculation(self) -> Dict[str, Any]:
        """åŸ·è¡Œå®Œæ•´çš„TLEè»Œé“è¨ˆç®—æµç¨‹"""
        self.logger.info("ğŸš€ é–‹å§‹Stage 1 TLEè»Œé“è¨ˆç®—æµç¨‹...")
        
        start_time = time.time()
        
        try:
